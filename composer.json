        "kartik-v/yii2-date-range": "^1.7",
        "kartik-v/yii2-helpers": "^1.3",
        "kartik-v/yii2-editable": "^1.7",
        "lito/yii2-cropper": "~2.0",
        "kartik-v/yii2-widget-select2": "^2.2",
        "froala/yii2-froala-editor": "^3.2",
        "lito/yii2-region": "^0.1.0"
